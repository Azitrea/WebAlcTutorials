<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>AdditionServlet</servlet-name>
        <servlet-class>web.config.AdditionServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>SubtractionServlet</servlet-name>
        <servlet-class>web.config.AdditionServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>MultiplyServlet</servlet-name>
        <servlet-class>web.config.AdditionServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>DivisionServlet</servlet-name>
        <servlet-class>web.config.AdditionServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>



    <servlet-mapping>
        <servlet-name>AdditionServlet</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SubtractionServlet</servlet-name>
        <url-pattern>/sub</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>MultiplyServlet</servlet-name>
        <url-pattern>/mul</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>DivisionServlet</servlet-name>
        <url-pattern>/div</url-pattern>
    </servlet-mapping>


</web-app>
